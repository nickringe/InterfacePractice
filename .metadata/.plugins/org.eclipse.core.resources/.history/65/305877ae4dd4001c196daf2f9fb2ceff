
public class Main {

	public static void main(String[] args) {
		Deer deer = new Deer(FoodType.Plants, 1000);
		deer.eat(FoodType.Plants, 100);
		deer.eat(FoodType.Meat, 101);
		
		Bush bush = new Bush(FoodType.Light, 25);
		bush.eat(FoodType.Light, 20);
		bush.eat(FoodType.Meat, 1000);
		bush.eat(FoodType.Light, 20);

	}

}
