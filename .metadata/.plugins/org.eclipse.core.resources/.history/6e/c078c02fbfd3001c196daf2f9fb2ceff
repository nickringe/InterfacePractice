
public class Deer implements Animal {

	@Override
	public void eat(String foodType, int calories) {
		// TODO Auto-generated method stub

	}
	
	@Override
	public boolean isSated(String foodType, int calories) {
		if (foodType == "Plants" && calories >= 1200) {
			return true;
		} else {
			return false;
		}
	}

	@Override
	public boolean isSated() {
		return false;
	}

}
